{
  "name": "weather-journal",
  "version": "0.1.0",
  "description": "An asynchronous web app that uses Web API and user data to dynamically update the UI.",
  "main": "index.html",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
